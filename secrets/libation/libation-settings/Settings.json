    "X": 255,
    "Y": 323,
    "Height": 1837,
    "Width": 2360,
    "IsMaximized": true
  },
  "ImportEpisodes": false,
  "DownloadEpisodes": false,
  "Panel2Collapsed": false,
  "CreateCueSheet": false,
  "LameDownsampleMono": false
}
