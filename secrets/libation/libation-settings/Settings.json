{
  "Books": "/data",
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/config/Log.log",
          "rollingInterval": "Month",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] (at {Caller}) {Message:lj}{NewLine}{Exception} {Properties:j}"
        }
      }
    ],
    "Using": [
      "Dinah.Core",
      "Serilog.Exceptions"
    ],
    "Enrich": [
      "WithCaller",
      "WithExceptionDetails"
    ]
  },
  "AutoScan": false,
  "DownloadCoverArt": true,
  "StripUnabridged": true,
  "FirstLaunch": false,
  "ProcessBookForm": {
    "X": 122,
    "Y": 122,
    "Height": 1832,
    "Width": 1300,
    "IsMaximized": false
  },
  "BadBook": "Retry",
